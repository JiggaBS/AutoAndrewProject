# ===========================================
# AutoMarket Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# cp .env.example .env

# -------------------------------------------
# SUPABASE (Required)
# Get these from: Lovable Cloud Settings or Supabase Dashboard
# -------------------------------------------
VITE_SUPABASE_URL="https://your-project-id.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="your-supabase-anon-key"
VITE_SUPABASE_PROJECT_ID="your-project-id"

# -------------------------------------------
# MULTIGESTIONALE API (Required for vehicle inventory)
# Get this from: Your Multigestionale dealer account
# -------------------------------------------
MULTIGESTIONALE_API_KEY="your-multigestionale-api-key"

# -------------------------------------------
# RESEND (Required for email notifications)
# Get this from: https://resend.com/api-keys
# -------------------------------------------
RESEND_API_KEY="re_xxxxxxxxxx"

# -------------------------------------------
# GOOGLE ANALYTICS (Optional - for tracking)
# Get this from: Google Analytics > Admin > Data Streams
# -------------------------------------------
VITE_GA_MEASUREMENT_ID="G-XXXXXXXXXX"

# -------------------------------------------
# SENTRY (Optional - for error tracking)
# Get this from: https://sentry.io
# -------------------------------------------
VITE_SENTRY_DSN=""
VITE_SENTRY_ENABLE_IN_DEV="false"
VITE_APP_VERSION="1.0.0"
